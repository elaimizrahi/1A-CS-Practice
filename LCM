#include <stdio.h>

int lcm(a,b, multiple){
    if(multiple%a==0 && multiple%b==0){
        return multiple;
    }
    multiple++;
    return lcm(a,b,multiple);
}

int main(void){
    int a = 0;
    int b;
    int multiple = 1;
    scanf("%d", &b);
    printf("%d", lcm(a,b,multiple));
        
    
    return 0;
}
